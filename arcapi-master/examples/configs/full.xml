  <nacm xmlns="urn:ietf:params:xml:ns:yang:ietf-netconf-acm">
    <write-default>permit</write-default>
    <rule-list>
      <name>admin-access</name>
      <group>admins</group>
      <group>root</group>
      <rule>
        <name>any-access</name>
        <action>permit</action>
      </rule>
    </rule-list>
    <rule-list>
      <name>any-group</name>
      <group>operators</group>
      <rule>
        <name>no-admin-nacm</name>
        <path>/nacm/rule-list[name='admin-access']</path>
        <action>deny</action>
      </rule>
      <rule>
        <name>user-info-change</name>
        <path>/ar-oc-aaa-aug:aaa/authentication/users/user[username='$USER']/config</path>
        <action>permit</action>
      </rule>
      <rule>
        <name>no-other-users</name>
        <path>/ar-oc-aaa-aug:aaa/authentication/users/user</path>
        <action>deny</action>
      </rule>
      <rule>
        <name>read-config</name>
        <path>/</path>
        <access-operations>create update delete</access-operations>
        <action>deny</action>
      </rule>
      <cmdrule xmlns="http://tail-f.com/yang/acm">
        <name>no-write-rule</name>
        <context>cli</context>
        <command>write</command>
        <access-operations>exec</access-operations>
        <action>deny</action>
        <comment>Deny all non-admin groups from config changes</comment>
      </cmdrule>
      <cmdrule xmlns="http://tail-f.com/yang/acm">
        <name>no-shell-rule</name>
        <context>cli</context>
        <command>bash</command>
        <access-operations>exec</access-operations>
        <action>deny</action>
        <comment>Deny all non-admin groups from shell access</comment>
      </cmdrule>
      <cmdrule xmlns="http://tail-f.com/yang/acm">
        <name>no-reset-config-rule</name>
        <context>cli</context>
        <command>reset-all-config</command>
        <access-operations>exec</access-operations>
        <action>deny</action>
        <comment>Deny all non-admin groups from being able to reset config</comment>
      </cmdrule>
      <cmdrule xmlns="http://tail-f.com/yang/acm">
        <name>no-cancel-reset-config-rule</name>
        <context>cli</context>
        <command>cancel-reset-all-config</command>
        <access-operations>exec</access-operations>
        <action>deny</action>
        <comment>Non-admin groups donot need access to cancel reset config</comment>
      </cmdrule>
    </rule-list>
  </nacm>
